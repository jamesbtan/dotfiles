# ?'.*youtube\.com.*': mpv
# ?'.*youtu\.be.*': mpv
video: mpv
audio: mpv

?'^http:\/\/': firefox-developer-edition
?'^https:\/\/': firefox-developer-edition
?'^www.': firefox-developer-edition
.html: firefox-developer-edition
.htm: firefox-developer-edition

.pdf: zathura
.epub: zathura
image: imv
.exe: wine
.eXe: wine
.csv: nvim->interm
text: nvim->interm

.docx: libreoffice
.xlsx: libreoffice

?'^logseq:': logseq

.apkg: anki

directory: nnn->interm
default: echo
